package problemDomain;

import java.io.Serializable;

public class BookingType implements Serializable {
	
	/**
	 * Initializes variables 
	 */
	private int id;
	private String name;
	private int setupTime;
	private int tearDownTime;
	
	public BookingType(){}

	/**
	 * @param id - id of booking
	 * @param name - name of booking
	 * @param setupTime - amount of time for setup
	 * @param tearDownTime - amount of time for tear down
	 */
	public BookingType(int id, String name, int setupTime, int tearDownTime) {
		this.id = id;
		this.name = name;
		this.setupTime = setupTime;
		this.tearDownTime = tearDownTime;
	}

	/**
	 * @param name - name of booking
	 * @param setupTime - amount of time for setup
	 * @param tearDownTime - amount of time for tear down
	 */
	public BookingType(String name, int setupTime, int tearDownTime) {
		this.name = name;
		this.setupTime = setupTime;
		this.tearDownTime = tearDownTime;
	}

	/**
	 * @return the id - id of booking
	 */
	public int getId() {
		return id;
	}

	/**
	 * The id to set
	 * @param id - id of booking
	 */
	public void setId(int id) {
		this.id = id;
	}

	/**
	 * @return the name - name of booking
	 */
	public String getName() {
		return name;
	}

	/**
	 * @return the setupTime - amount of time for setup
	 */
	public int getSetupTime() {
		return setupTime;
	}

	/**
	 * @return the tearDownTime - amount of time for tear down
	 */
	public int getTearDownTime() {
		return tearDownTime;
	}

	/**
	 * The name to set
	 * @param name - name of booking 
	 */
	public void setName(String name) {
		this.name = name;
	}

	/**
	 * The setupTime to set
	 * @param setupTime -amount of time for setup
	 */
	public void setSetupTime(int setupTime) {
		this.setupTime = setupTime;
	}

	/**
	 * The tearDownTime to set
	 * @param tearDownTime - amount of time for tear down
	 */
	public void setTearDownTime(int tearDownTime) {
		this.tearDownTime = tearDownTime;
	}
}
