package problemDomain;

import java.io.Serializable;

public class Employee implements Serializable {
	
	/**
	 * 
	 */
	private static final long serialVersionUID = 2381322733587381992L;
	/**
	 * Initializes variables
	 */
	private int id = 0;
	private String username;
	private String password;
	private String givenName;
	private String surname;
	private int employeeLevel;

	public Employee(){}
	
	/**
	 * @param id - employee id
	 * @param username - employee login username
	 * @param password - employee login password
	 * @param givenName - employee given name
	 * @param surname - employee surname
	 * @param employeeLevel - employee level, the amount of access they have to system
	 */
	public Employee(int id, String username, String password, String givenName, String surname, int employeeLevel) {
		this.id = id;
		this.username = username;
		this.password = password;
		this.givenName = givenName;
		this.surname = surname;
		this.employeeLevel = employeeLevel;
	}
	
	/**
	 * @param id - employee id
	 * @param username - employee login username
	 * @param givenName - employee login password
	 * @param surname - employee surname
	 * @param employeeLevel - employee level, the amount of access they have to system
	 */
	public Employee(int id, String username, String givenName, String surname, int employeeLevel) {
		this.id = id;
		this.username = username;
		this.givenName = givenName;
		this.surname = surname;
		this.employeeLevel = employeeLevel;
	}
	
	/**
	 * @param username - employee login username
	 * @param password - employee login password
	 * @param givenName - employee given name
	 * @param surname - employee surname
	 * @param employeeLevel - employee level, the amount of access they have to system
	 */
	public Employee(String username, String password, String givenName, String surname, int employeeLevel)
	{
		this.id = 0;
		this.username = username;
		this.password = password;
		this.givenName = givenName;
		this.surname = surname;
		this.employeeLevel = employeeLevel;
	}

	/**
	 * @return the id - employee id
	 */
	public int getId() {
		return id;
	}

	/**
	 * @return the username - employee login username
	 */
	public String getUsername() {
		return username;
	}

	/**
	 * @return the password - employee login password
	 */
	public String getPassword() {
		return password;
	}

	/**
	 * @return the givenName - employee given name
	 */
	public String getGivenName() {
		return givenName;
	}

	/**
	 * @return the surname - employee surname
	 */
	public String getSurname() {
		return surname;
	}

	/**
	 * The id to set
	 * @param id - employee id
	 */
	public void setId(int id) {
		this.id = id;
	}

	/**
	 * The username to set
	 * @param username - employee login username
	 */
	public void setUsername(String username) {
		this.username = username;
	}

	/**
	 * The password to set
	 * @param password - employee login password
	 */
	public void setPassword(String password) {
		this.password = password;
	}

	/**
	 * The givenName to set
	 * @param givenName - employee given name
	 */
	public void setGivenName(String givenName) {
		this.givenName = givenName;
	}

	/**
	 * The surname to set
	 * @param surname - employee surname
	 */
	public void setSurname(String surname) {
		this.surname = surname;
	}
	
	/**
	 * The employeeLevel to get
	 * @return the employee Level
	 */
	public int getEmployeeLevel()
	{
		return employeeLevel;
	}

	/**
	 * The employeeLevel to set
	 * @param employeeLevel - employee Level
	 */
	public void setEmployeeLevel(int employeeLevel)
	{
		this.employeeLevel = employeeLevel;
	}

	
	
}
