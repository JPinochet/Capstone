package exceptions;

/**
 * @author cantleyc
 *
 */
public class GivenNameInvalidException extends Exception {
	private static final long serialVersionUID = -5660174830670926590L;

	/**
	 *  Initializes Exception
	 */
	public GivenNameInvalidException() {
	}

	/**
	 *Gets the specific message for the exception
	 * @param message is the message that is thrown for the exception
	 */
	public GivenNameInvalidException(String message) {
		super(message);		
	}
	
	/**
	 * Gets the specific cause for the exception
	 * @param cause is the cause for the reason an exception is thrown
	 */
	public GivenNameInvalidException(Throwable cause) {
		super(cause);
	}
	
	/**
	 * Gets the specific message and cause for the exception that is thrown
	 * @param message is the message that is thrown for the exception
	 * @param cause is the cause for the reason an exception is thrown
	 */
	public GivenNameInvalidException(String message, Throwable cause) {
		super(message, cause);
	}	
}
