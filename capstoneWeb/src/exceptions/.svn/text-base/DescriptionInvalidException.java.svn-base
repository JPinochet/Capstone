package exceptions;

/**
 * @author 432873
 *
 */
public class DescriptionInvalidException extends Exception {
	private static final long serialVersionUID = 3487431483272003796L;
	
	/**
	 * Initializes Exception
	 */
	public DescriptionInvalidException()
	{
		
	}
	
	/**
	 * Gets the specific message for the exception
	 * @param message is the message that is thrown for the exception
	 */
	public DescriptionInvalidException(String message)
	{
		super(message);
	}

	/**
	 * Gets the specific cause for the exception
	 * @param cause is the cause for the reason an exception is thrown
	 */
	public DescriptionInvalidException(Throwable cause)
	{
		super(cause);
	}
	
	/**
	 * Gets the specific message and cause for the exception that is thrown
	 * @param message is the message that is thrown for the exception
	 * @param cause is the cause for the reason an exception is thrown
	 */
	public DescriptionInvalidException(String message, Throwable cause)
	{
		super(message, cause);
	}
}
