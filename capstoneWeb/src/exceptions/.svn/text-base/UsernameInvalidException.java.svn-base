package exceptions;

/**
 * @author cantleyc
 *
 */
public class UsernameInvalidException extends Exception {
	private static final long serialVersionUID = 7399395941614678571L;

	/**
	 * Initializes Exception
	 */
	public UsernameInvalidException() {
	}
	
	/**
	 * Gets the specific message for the exception
	 * @param message is the message that is thrown for the exception
	 */
	public UsernameInvalidException(String message) {
		super(message);
	}
	
	/**
	 * Gets the specific cause for the exception
	 * @param cause is the cause for the reason an exception is thrown
	 */
	public UsernameInvalidException(Throwable cause) {
		super(cause);
	}
	
	/**
	 * Gets the specific message and cause for the exception that is thrown
	 * @param message is the message that is thrown for the exception
	 * @param cause is the cause for the reason an exception is thrown
	 */
	public UsernameInvalidException(String message, Throwable cause) {
		super(message, cause);
	}
	
}
