package exceptions;

/**
 * @author 432873
 *
 */
public class PostalCodeInvalidException extends Exception {
	private static final long serialVersionUID = -820287400080148880L;

	/**
	 * Initializes Exception
	 */
	public PostalCodeInvalidException()
	{
		
	}
	
	/**
	 * Gets the specific message for the exception
	 * @param message is the message that is thrown for the exception
	 */
	public PostalCodeInvalidException(String message)
	{
		super(message);
	}
	
	/**
	 * Gets the specific cause for the exception
	 * @param cause is the cause for the reason an exception is thrown
	 */
	public PostalCodeInvalidException(Throwable cause)
	{
		super(cause);
	}
	
	/**
	 * Gets the specific message and cause for the exception that is thrown
	 * @param message is the message that is thrown for the exception
	 * @param cause is the cause for the reason an exception is thrown
	 */
	public PostalCodeInvalidException(String message, Throwable cause)
	{
		super(message, cause);
	}
}
