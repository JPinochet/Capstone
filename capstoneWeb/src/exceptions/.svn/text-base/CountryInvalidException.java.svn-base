package exceptions;

/**
 * @author 432873
 *
 */
public class CountryInvalidException extends Exception {
	private static final long serialVersionUID = -3248072190289301486L;
	
	/**
	 * Initializes Exception
	 */
	public CountryInvalidException()
	{
		
	}

	/**
	 * Gets the specific message for the exception
	 * @param message is the message that is thrown for the exception
	 */
	public CountryInvalidException(String message)
	{
		super(message);
	}
	
	/**
	 * Gets the specific cause for the exception
	 * @param cause is the cause for the reason an exception is thrown
	 */
	public CountryInvalidException(Throwable cause)
	{
		super(cause);
	}
	
	/**
	 * Gets the specific message and cause for the exception that is thrown
	 * @param message is the message that is thrown for the exception
	 * @param cause is the cause for the reason an exception is thrown
	 */
	public CountryInvalidException(String message, Throwable cause)
	{
		super(message, cause);
	}
}
