package problemDomain;

public class Catering {
	private int id = -1;
	private String chargeName;
	private double charge;

	/**
	 * @param id
	 * @param chargeName
	 * @param charge
	 */
	public Catering(int id, String chargeName, double charge) {
		this.id = id;
		this.chargeName = chargeName;
		this.charge = charge;
	}

	/**
	 * @param chargeName
	 * @param charge
	 */
	public Catering(String chargeName, double charge) {
		this.chargeName = chargeName;
		this.charge = charge;
	}

	/**
	 * @return the chargeName
	 */
	public String getChargeName() {
		return chargeName;
	}

	/**
	 * @return the charge
	 */
	public double getCharge() {
		return charge;
	}

	/**
	 * @param chargeName
	 *            the chargeName to set
	 */
	public void setChargeName(String chargeName) {
		this.chargeName = chargeName;
	}

	/**
	 * @param charge
	 *            the charge to set
	 */
	public void setCharge(double charge) {
		this.charge = charge;
	}

	/**
	 * @return the id
	 */
	public int getId() {
		return id;
	}

	/**
	 * @param id
	 *            the id to set
	 */
	public void setId(int id) {
		this.id = id;
	}
}
